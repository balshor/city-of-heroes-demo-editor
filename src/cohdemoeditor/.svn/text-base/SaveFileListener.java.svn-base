package cohdemoeditor;

/**
 * This interface describes an object that wishes to receive events when the
 * save file of a DemoCommandList is changed.
 * 
 * @author Darren Lee
 */
public interface SaveFileListener {

	/**
	 * Called when a DemoCommandList has its saveFile property changed.
	 * 
	 * @param source
	 */
	public void saveFileChanged(DemoCommandList source);
}
